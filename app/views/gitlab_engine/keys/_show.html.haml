%tr
  %td
    = link_to gitlab_engine.key_path(key) do
      %p
        %strong= key.title
  %td
    %span.cgray
      Added
      = time_ago_in_words(key.created_at)
      ago
  %td
    = link_to 'Remove', gitlab_engine.key_path(key), {:data => {:confirm => 'Are you sure?'}, :method => :delete, :class => "btn small danger delete-key right"}

