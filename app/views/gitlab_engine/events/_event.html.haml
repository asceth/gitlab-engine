- if event.allowed?
  - if event.issue?
    .event_feed
      = render "gitlab_engine/events/event_issue", :event => event

  - elsif event.merge_request?
    .event_feed
      = render "gitlab_engine/events/event_merge_request", :event => event

  - elsif event.push?
    .event_feed
      = render "gitlab_engine/events/event_push", :event => event

