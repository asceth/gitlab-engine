= render "repositories/head"
- if can? current_user, :admin_project, @project
  .alert-message.block-message
    Deploy keys allow read-only access to repository.
    = link_to gitlab_engine.new_project_deploy_key_path(@project), :class => "btn small", :title => "New Deploy Key" do
      Add Deploy Key

- if @keys.any?
  %table
    - @keys.each do |key|
      = render(:partial => 'gitlab_engine/deploy_keys/show', :locals => {:key => key})
